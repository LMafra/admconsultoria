<section class="fale_conosco" id="contato">
	<div class="wrapper">
		<h1>FALE CONOSCO</h1>
    <p>Agende seu atendimento com nossos Analistas de Negócios,
    para podermos oferecer-lhe o melhor das nossas soluções.</p>
    
    <ul class="flex-container">
      <li>
      <%= simple_form_for @contact_form , remote: true do |f| %>

            <div class="formulario">
              <%= f.text_field :name, placeholder:"Nome" %>
            </div>

            <div class="formulario">
              <%= f.email_field :email, placeholder:"Email" %>
            </div>

            <div class="formulario">
              <%= f.text_field :phone, placeholder:"Telefone" %>
            </div>

            <div class="formulario">
              <%= f.text_field :company_name, placeholder:"Nome da Empresa" %>
            </div>

            <div class="formulario">
              <%= f.text_area :message,placeholder:"Como podemos ajudar?" %>
            </div>

            <div class="envio">
              <%= f.submit 'Enviar', class: 'button' %>
            </div>
        <% end %>
        
      </li>
      <li>
      	<div id="map"></div>
      </li>
    </ul>
	</div>
</section>


